type t_calc_data_rec is record
    (
        calculation_name             varchar2(200 char),
        -- сугубо входные данные
        collection_code              number(30),
        delivery_country_code        number(30),
        brand_code                   number(30),
        age_code                     number(30),
        gender_code                  number(30),
        gom_code                     number(30),
        category_code                number(30),
        class_code                   number(30),
        subclass_code                number(30),
        sop_code                     number(30),
        calculation_date             date,
        currency_type_code           number(30),
        currency_code                number(30),
        currency_alt_code            number(30),          -- альтернативная валюта, еще известная как default
        production_region_code       number(30),          -- Выбранный регион производства
        purchase_price               number,              -- Значения ЗЦ, валюты и VAT Type с приоритетного региона берутся
        purchase_price_currency_code number(30),
        purchase_price_vat_type      number(1),
        calculation_method           number(1),
        calculate_delivery           number(1),           -- надо ли рассчитывать доствку (0/1)
        avia_delivery                number(1),           -- надо ли считать доставку авиа
        avia_ratio                   number(9,2),         -- Доля ассортимента, доставляемого avia
        transport_type_code          number(30),          -- Код транспорта
        -- входные/выходные данные
        weight                       number,
        volume                       number,
        licensor_code                number(30),
        royalty_rate                 number,
        royalty_base                 number(1),
        effective_date               date,
        exchange_rate_value          number,
        lc_net_main_currency         number,
        hs_code                      number(30),
        rrp_gross                    number(30,4),
        vat_rate                     number,
        calculate_customs            number(1),           -- надо ли рассчитывать таможенную стоимость (0/1)
        -- сугубо выходные данные
        lc_net_default_currency      number,
        rrp_net                      number(30,4),
        planned_mup                  number,              -- отношение RRP net / LC net
        target_mup                   number,              -- плановый маркап из справочника
        err_text                     varchar2(4000 char),
        rrp_usd                      number(30,4),
        vat_cost_main_currency       number,
        vat_cost_alt_currency        number,
        calc_custom_base             number(1)
    );