-- merge с delete нужно отличать от merge с последующим delete
begin
    merge 
    into 
        (
            select 
                * 
            from 
                dual
        ) d1 
    using 
        (
            select 
                * 
            from 
                dual
        ) d2 
    on 
        (d1.dummy = d2.dummy)
    when matched then 
        update 
        set 
            dummy = 'Y';
    delete 
    from 
        dual 
    where 
        dummy <> 'Y';
end;
