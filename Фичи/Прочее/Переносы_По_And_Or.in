-- ¬ыражение в where должно перенестись по and, а в select остатьс€ нетронутым
select
  case when dummy = 'X' and dummy <> 'Y' and sysdate > 1000 then 1 else 0 end    
from
  dual
where
  dummy = 'X' and dummy <> 'Y' and sysdate > 1000;

-- ¬ыражение в where должно перенестись по and, а в set остатьс€ нетронутым
update dual set
  dummy = case when dummy = 'X' and dummy <> 'Y' and sysdate > 1000 then '1' else '0' end    
where
  dummy = 'X' and dummy <> 'Y' and sysdate > 1000;

