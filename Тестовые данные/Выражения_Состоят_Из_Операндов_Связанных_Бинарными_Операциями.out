select
    1 + 2,
    1 + -2,
    'a' * 4,
    5 / 0,
    1 || b + c
from
    dual
where
    a = 1 
    and b < 2 
    and c > 3 
    and d <= 4 
    and e >= 5 
    and f <> 6 
    and g != 7 
    and h ^= 8 
    and i like 9 
    and j not like 10
    and k in (11, 12, 13) 
    and (l, m, n) not in (select * from dual) 
    or o between 14 and 15
